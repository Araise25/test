{
  "name": "crypto-tracker",
  "version": "1.0.0",
  "description": "A Streamlit application for tracking cryptocurrency prices",
  "author": {
    "name": "RushiChaganti",
    "email": "your.email@example.com",
    "github": "Araise25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Araise25/crypto-tracker",
    "branch": "main"
  },
  "dependencies": {
    "required": [
      "git",
      "python3",
      "pip"
    ],
    "optional": []
  },
  "install": {
    "command": "git clone https://github.com/Araise25/crypto-tracker.git",
    "post_install": [
      "cd crypto-tracker",
      "pip install -r requirements.txt"
    ],
    "supported_platforms": ["windows", "linux", "macos"]
  },
  "run": {
    "commands": [
      "streamlit run app.py"
    ]
  },
  "uninstall": {
    "command": "rm -rf ${INSTALL_DIR}",
    "pre_uninstall": "cleanup.sh"
  },
  "tags": [
    "cryptocurrency",
    "finance",
    "streamlit",
    "dashboard"
  ],
  "license": "MIT",
  "documentation": {
    "readme": "README.md",
    "usage": "docs/usage.md"
  },
  "configuration": {
    "env_variables": [
      "CRYPTO_API_KEY"
    ]
  },
  "metadata": {
    "created_at": "2024-03-24",
    "last_updated": "2024-03-24",
    "minimum_araise_version": "1.0.0",
    "category": "finance-tools"
  }
}
